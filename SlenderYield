-- FLING
local flingConn
local spin

local function findPlayer(txt)
	txt = txt:lower()
	for _,p in pairs(Players:GetPlayers()) do
		if p ~= LP and (
			p.Name:lower():sub(1,#txt) == txt or
			p.DisplayName:lower():sub(1,#txt) == txt
		) then
			return p
		end
	end
end

local function startFling(target)
	local char = LP.Character
	if not char then return end
	local hrp = char:WaitForChild(kaKJAijzWgiLWtHWGcbteJMEldWUDaFToviYaMlnUkvRmtismhocAzBduNuGPKbQJjnwYwJsoIryPyGjPnyiGnWgyMs('xPwyaWfTTWraIpOiOIwZQrewrNsddjzXFZEFUNkEqLNWWdTFQJslEQlSHVtYW5vaWRSb290UGFydA=='))

	spin = Instance.new(kaKJAijzWgiLWtHWGcbteJMEldWUDaFToviYaMlnUkvRmtismhocAzBduNuGPKbQJjnwYwJsoIryPyGjPnyiGnWgyMs('sARFztOyWDGURPacUkFoETgBSFsMYNBOTppaZuYoVOgYkdvxtyBEzKGQm9keUFuZ3VsYXJWZWxvY2l0eQ=='), hrp)
	spin.MaxTorque = Vector3.new(math.huge,math.huge,math.huge)
	spin.AngularVelocity = Vector3.new(0,60,0)
	spin.P = 5000

	flingConn = RunService.Heartbeat:Connect(function()
		if not target.Character then return end
		local thrp = target.Character:FindFirstChild(kaKJAijzWgiLWtHWGcbteJMEldWUDaFToviYaMlnUkvRmtismhocAzBduNuGPKbQJjnwYwJsoIryPyGjPnyiGnWgyMs('EErMhcSdfFSOXbbYqUHYrAkyvCZyBjUexYasHitVtWaNqaCGcXUbRHGSHVtYW5vaWRSb290UGFydA=='))
		if not thrp then return end

		hrp.CFrame = thrp.CFrame
		hrp.AssemblyLinearVelocity =
			Vector3.new(
				math.random(-3000,3000),
				math.random(2000,4000),
				math.random(-3000,3000)
			)
	end)
end

-- COMANDOS
box.FocusLost:Connect(function(enter)
	if not enter then return end
	local cmd = box.Text
	box.Text = kaKJAijzWgiLWtHWGcbteJMEldWUDaFToviYaMlnUkvRmtismhocAzBduNuGPKbQJjnwYwJsoIryPyGjPnyiGnWgyMs('EZMRDDoIWqvaynrwlALIdaJNQBRWvsipTtonMftiIeagnMXvDzIOZnI')

	local args = cmd:split(kaKJAijzWgiLWtHWGcbteJMEldWUDaFToviYaMlnUkvRmtismhocAzBduNuGPKbQJjnwYwJsoIryPyGjPnyiGnWgyMs('zKDPZICCsMVuaOthnFkiBdJHeFsRTyDfeGmphApNVAUeboqZfVlQBnxIA=='))

